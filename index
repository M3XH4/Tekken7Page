	<div id="box">
		<img src="https://wallpaperaccess.com/full/7019872.jpg	" alt="Tekken Logo" width="250px" height="100px"> <!--The Logo-->
		<form> 																		
			<label for="skill">What's Your Fighting Style:</label>
			<select id="skill" name="Skill List">
				<option value="mobility">Mobility</option>								<!--Master Raven!-->
				<option value="grappling">Grappling</option>							<!--King!-->
				<option value="strength">Strength</option>								<!--Jin Kazama-->
				<option value="combomaster">Combo Master</option>						<!--Eddy-->
				<option value="counter">Counter Hitter</option>							<!--Steve!-->
			</select>
			<button type="button" onclick="genchar()">Generate</button> 				<!--The Button Which Will Run The Generate Character Function-->
			<button type="button" onclick="showAllChar()">Show All</button>				<!--The Bittpm Which Will Run The Show All Character Function-->
		</form>
		<table id="charlist">
			<tr>
				<td></td>
				<!--Images of Each Character-->
				<td class="charEddy"><img src="https://static.wikia.nocookie.net/tekken/images/5/56/Eddy_Gordo_T7.jpg/revision/latest/scale-to-width-down/350?cb=20170408103724&path-prefix=en" alt="EddyPhoto"></td>
				<td class="charSteve"><img src="https://static.wikia.nocookie.net/tekken/images/6/6b/T7FR_Steve.jpg/revision/latest/scale-to-width-down/350?cb=20210822074545&path-prefix=en" alt="StevePhoto"></td>
				<td class="charKing"><img src="https://static.wikia.nocookie.net/tekken/images/4/42/T7FR_King.jpg/revision/latest?cb=20211120091042&path-prefix=en" alt="KingPhoto"></td>
				<td class="charMR"><img src="https://static.wikia.nocookie.net/tekken/images/1/1d/MasterRavenT7.jpg/revision/latest?cb=20191018004040&path-prefix=en" alt="MRPhoto"></td>

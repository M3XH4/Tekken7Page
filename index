	<script>
		//This Is To Get The Class And Put It Into Their Specific Variables
		var eddyPick = document.getElementsByClassName('charEddy');
		var stevePick = document.getElementsByClassName('charSteve');
		var kingPick = document.getElementsByClassName('charKing');
		var mrPick = document.getElementsByClassName('charMR');
		var jinPick = document.getElementsByClassName('charJin');

		
		function hide()					//This Function Will Hide The Specified Elements
		{
			eddyFunctionNone();			//Functions That Hides The Elements
			steveFunctionNone();
			kingFunctionNone();
			mrFunctionNone();
			jinFunctionNone();
		}
		window.onload = hide; 			//Once The Page Finishes Loading It Will Hide The Elements

		function character(name, fightstyle, rate)		// An Object That Specifies The Name, Fighting Style, And Ratings
		{
			this.name = name;
			this.fightstyle = fightstyle;
			this.rate = rate;
		}

		function genchar() 								//This Function will Generate The Character Based On What The User Chooses In The Form
		{
			var skills = document.getElementById("skill").value;	//To Get The Value Of The Select Form or Their Option
			if(skills == "combomaster") 							//If the Value Is Combo Master
			{
																	//This Is To Specify That Only One Will Display and The Others Will Still Stay Hidden
				steveFunctionNone();								//The Hidden Function
				kingFunctionNone();									
				mrFunctionNone();
				jinFunctionNone();

				eddyFunctionBlock();								//The Displayed Function
				alert("I Recommend Eddy Gordo");					//An Alert Box That Says What The User's Picked Character is
			}
			else if (skills == "counter")							//If The Value Is Counter Hitter
			{
				eddyFunctionNone();
				kingFunctionNone();
				mrFunctionNone();
				jinFunctionNone();
				steveFunctionBlock();
				alert("I Recommend Steve Fox");
			}
			else if (skills == "grappling")							//If The Value Is Grappler
			{
				eddyFunctionNone();
				steveFunctionNone();
				mrFunctionNone();
				jinFunctionNone();
				kingFunctionBlock();
				alert("I Recommend King")
			}
			else if (skills == "mobility")							//If The Value Is Mobility
			{
				eddyFunctionNone();
				steveFunctionNone();
				kingFunctionNone();
				jinFunctionNone();
				mrFunctionBlock();
				alert("I Recommend Master Raven")
			}
			else if (skills == "strength")							//If The Value is Strength
			{
				eddyFunctionNone();	
				steveFunctionNone();
				kingFunctionNone();
				mrFunctionNone();
				jinFunctionBlock();
				alert("I Recommend Jin Kazama");
			}
		}
		function showAllChar()										// This Function Will Show All The Characters
		{
			eddyFunctionNull();										//To Show The Characters
			steveFunctionNull();					
			kingFunctionNull();
			mrFunctionNull();
			jinFunctionNull();
		}

		/*				Eddy's Information And Functions 							*/
		var Eddy = new character("Eddy Gordo", "Capoeira", 6);						//Their Information in an Object Value
		document.getElementById("eddyName").innerHTML = Eddy.name;					//To Display It's Name
		document.getElementById("eddyStyle").innerHTML = Eddy.fightstyle;			//To Display It's Fighting Style
		document.getElementById("eddyRate").innerHTML = Eddy.rate + "/10";			//To Display It's Rating
		function eddyFunctionBlock() 												//This Function Will Display The Character and it's Information
		{
			for (var x = 0; x < 4; x++)												//Looping Because of Class since It Is Like An Array Whenever You Want To Edit It
			{
				eddyPick[x].style.display = "block";								//To Display The Elements
			}
		}
		function eddyFunctionNone() 												//This Function Will Hide The Character and It's Information
		{
			for (var x = 0; x < 4; x++)							
			{
				eddyPick[x].style.display = "none";									//To Hide The Elements				
			}
		}
		function eddyFunctionNull() 												//To Remove It's Styles
		{
			for (var x = 0; x < 4; x++)
			{
				eddyPick[x].style.display = null;									//Null So That It Reverts Back To It's Original Display 'flexbox'
			}																		//P.S, It Cannot Display Back To Block Because It Will Become Messy
		}

		/*				Steve's Information And Functions 							*/
		var Steve = new character("Steve Fox", "Boxing", 7);
		document.getElementById("steveName").innerHTML = Steve.name;
		document.getElementById("steveStyle").innerHTML = Steve.fightstyle ;
		document.getElementById("steveRate").innerHTML = Steve.rate + "/10";
		function steveFunctionBlock() 
		{
			for (var x = 0; x < 4; x++)
			{
				stevePick[x].style.display = "block";
			}
		}
		function steveFunctionNone() 
		{
			for (var x = 0; x < 4; x++)
			{
				stevePick[x].style.display = "none";
			}
		}
		function steveFunctionNull() 
		{
			for (var x = 0; x < 4; x++)
			{
				stevePick[x].style.display = null;
			}
		}

		/*				King's Information And Functions 							*/
		var King = new character("King", "Pro Wrestling", 8)
		document.getElementById("kingName").innerHTML = King.name;
		document.getElementById("kingStyle").innerHTML = King.fightstyle ;
		document.getElementById("kingRate").innerHTML = King.rate + "/10";
		function kingFunctionBlock() 
		{
			for (var x = 0; x < 4; x++)
			{
				kingPick[x].style.display = "block";
			}
		}
		function kingFunctionNone() 
		{
			for (var x = 0; x < 4; x++)
			{
				kingPick[x].style.display = "none";
			}
		}
		function kingFunctionNull() 
		{
			for (var x = 0; x < 4; x++)
			{
				kingPick[x].style.display = null;
			}
		}

		/*				Master Raven's Information And Functions 							*/
		var MR = new character("Master Raven", "Ninjutsu", 7)
		document.getElementById("mrName").innerHTML = MR.name;
		document.getElementById("mrStyle").innerHTML = MR.fightstyle ;
		document.getElementById("mrRate").innerHTML = MR.rate + "/10";
		function mrFunctionBlock() 
		{
			for (var x = 0; x < 4; x++)
			{
				mrPick[x].style.display = "block";
			}
		}
		function mrFunctionNone() 
		{
			for (var x = 0; x < 4; x++)
			{
				mrPick[x].style.display = "none";
			}
		}
		function mrFunctionNull() 
		{
			for (var x = 0; x < 4; x++)
			{
				mrPick[x].style.display = null;
			}
		}

		/*				Jin Kazama's Information And Functions 							*/
		var Jin = new character("Jin Kazama", "Mishima-Ryu Karate", 9)
		document.getElementById("jinName").innerHTML = Jin.name;
		document.getElementById("jinStyle").innerHTML = Jin.fightstyle ;
		document.getElementById("jinRate").innerHTML = Jin.rate + "/10";
		function jinFunctionBlock() 
		{
			for (var x = 0; x < 4; x++)
			{
				jinPick[x].style.display = "block";
			}
		}
		function jinFunctionNone() 
		{
			for (var x = 0; x < 4; x++)
			{
				jinPick[x].style.display = "none";
			}
		}
		function jinFunctionNull() 
		{
			for (var x = 0; x < 4; x++)
			{
				jinPick[x].style.display = null;
			}
		}
	</script>
</body>
